{
  "name": "forms-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/forms-lib/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "implicitDependencies": ["mui-lib"],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/forms-lib/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/azure-blue.css",
          "libs/forms-lib/src/lib/styles.scss"
        ],
        "port": 4400,
        "configDir": "libs/forms-lib/.storybook",
        "browserTarget": "forms-lib:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/rose-red.css",
          "libs/forms-lib/src/lib/styles.scss"
        ],
        "outputDir": "dist/storybook/forms-lib",
        "configDir": "libs/forms-lib/.storybook",
        "browserTarget": "forms-lib:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c libs/forms-lib/.storybook --url=http://localhost:4400"
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "dependsOn": ["build-storybook"],
      "options": {
        "buildTarget": "forms-lib:build-storybook",
        "staticFilePath": "dist/storybook/forms-lib",
        "spa": true
      },
      "configurations": {
        "ci": {
          "buildTarget": "forms-lib:build-storybook:ci"
        }
      }
    }
  }
}
